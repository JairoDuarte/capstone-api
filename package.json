{
  "name": "api",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "docker:test": "npm i && npm test",
    "commit": "npm run lint",
    "test": "NODE_ENV=testing jest --forceExit",
    "start": "node_modules/kill-port/cli.js 4000 && ENV_PATH=.env.development  NODE_ENV=development nodemon --exec babel-node ./server.js",
    "start:dev": "./node_modules/.bin/nodemon --exec babel-node ./server.js",
    "lint": "eslint '**/*.js'",
    "start:test": "NODE_ENV=testing babel-node ./test/integration/testsingle.js",
    "lint:fix": "eslint '**/*.js' --fix"
  },
  "jest": {
    "testEnvironment": "node",
    "setupFilesAfterEnv": [
      "<rootDir>/test/integration/setup.js"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run commit",
      "pre-push": "npm run commit"
    }
  },
  "dependencies": {
    "axios": "^0.21.1",
    "bcrypt": "^3.0.4",
    "bodymen": "^1.1.0",
    "chromedriver": "^2.46.0",
    "cookie-parser": "~1.4.3",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^6.2.0",
    "express": "~4.16.0",
    "express-session": "^1.15.6",
    "http-errors": "~1.6.2",
    "jade": "~1.11.0",
    "jsonwebtoken": "^8.5.0",
    "mongoose": "^5.7.5",
    "mongoose-keywords": "^0.4.0",
    "morgan": "~1.9.0",
    "passport": "^0.4.0",
    "passport-facebook": "^3.0.0",
    "passport-facebook-token": "^3.3.0",
    "passport-http-bearer": "^1.0.1",
    "passport-jwt": "^4.0.0",
    "redis-sessions": "^2.1.0",
    "request": "^2.88.0",
    "request-promise": "^4.2.4",
    "selenium-webdriver": "^4.0.0-alpha.1",
    "socket.io": "^2.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.3.4",
    "@babel/node": "^7.2.2",
    "@babel/polyfill": "^7.2.5",
    "@babel/preset-env": "^7.3.4",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.4.2",
    "elint": "^1.10.0",
    "enzyme": "^3.9.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jest": "^22.3.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "jest": "^24.1.0",
    "kill-port": "^1.3.2",
    "mongodb": "^3.1.13",
    "nodemon": "^1.18.10",
    "regenerator-runtime": "^0.13.1",
    "superagent": "^4.1.0",
    "supertest": "^3.4.2"
  }
}
